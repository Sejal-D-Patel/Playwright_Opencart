<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Open Cart Test Regression PW Suite" thread-count ="2" parallel ="tests" verbose="4">

 <!--for parallel execution thread-count ="2"  parallel ="tests" 
 verbose="4" means how many logs from the testNG you want to generate on the console-->

<listeners>
     <listener class-name="com.qa.opencart.listeners.ExtentReportListener"/>
</listeners> 
  <test name="Open Cart Home Page Test">
      <classes>
      <class name="com.qa.opencart.tests.HomePageTest"/>
      </classes>
  </test> <!-- Test -->    
   
    <test name="Open Cart Login Page Test">
      <classes>
      <class name="com.qa.opencart.tests.LoginTest"/>
      </classes>
  </test>
 </suite> <!-- Suite --> 